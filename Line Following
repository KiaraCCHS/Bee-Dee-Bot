#include <PRIZM.h>    
PRIZM prizm;         

void setup() {

  prizm.PrizmBegin();        
  prizm.setServoSpeed(1,25);  

}

void loop() {     // repeat in a loop

  prizm.setServoPosition(1,80);  
  delay(500);                   
                                 
  prizm.setServoPosition(1,55);    
  delay(500);                    
 
}
